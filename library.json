    "repository": {
        "type": "git",
        "url": "https://github.com/nodeiot/remoteIO"
    },
    "keywords": ["remoteio", "remote", "iot", "cloud", "communication"],
    "authors": {
        "name": "gkraemer-niot",
        "email": "guilherme.kraemer@nodeiot.com.br",
        "maintainer": true
    },
    "frameworks": "arduino",
    "platforms": "espressif32",
    "dependencies": [
        {
            "owner": "bblanchon",
            "name": "ArduinoJson",
            "version": "7.1.0"
        },
        {
            "owner": "Links2004",
            "name": "WebSockets",
            "version": "2.4.2"
        },
        {
            "owner": "me-no-dev",
            "name": "ESP Async WebServer",
            "version": "1.2.4"
        }
    ],
    "export": {
        "exclude": [
            "src/main.cpp"
        ]
    }
